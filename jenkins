
pipeline {
    agent any

    stages {
        stage('build'){
            steps{
                echo 'This is Build stage'
            }
        }
        stage ('testing'){
            steps{
                  echo 'This is tesing stage'
                }
                
            }
        
            
        stage (deploy){
            steps{
                        echo "this is deploy stage"
                    }
            }
    }            
                    
                
            
       
}        
    

